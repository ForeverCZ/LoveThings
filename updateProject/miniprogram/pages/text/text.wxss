page {
  padding: 5rpx;
  background-color: rgb(250, 210, 210);
}

.content-wrap {
  height: 100vh;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
}

.photo-wap {
  padding-top: 150rpx;
}

.result-text-wrap {
  margin-top: 40rpx;
}

.result-text-wrap scroll-view {
  width: 700rpx;
  height: 500rpx;
  text-align: center;
  padding: 0;
}

text {
  text-align: left;
  color: rgb(48, 46, 46);
  font-weight: bold;
}

.photo-wap .photo-img {
  width: 150rpx;
  height: 150rpx;
}

.btn-copy button {
  margin-bottom: 70rpx;
}
